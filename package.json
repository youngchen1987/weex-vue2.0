{
  "name": "weex-frame",
  "description": "a frame for weex ~",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "keywords": [
    "weex",
    "vue"
  ],
  "scripts": {
    "init": "node build/init.js",
    "dev": "webpack --watch",
    "copy": "rm -rf android/app/src/main/assets/* && cp -vrf dist/weex/* android/app/src/main/assets/&& rm -rf ios/assets/* && cp -vrf dist/weex/* ios/assets/",
    "serve": "node build/ip.js && serve -p 8080",
    "build": "webpack",
    "debug": "weex-devtool"
  },
  "dependencies": {
    "qs": "^6.5.1",
    "vue": "^2.4.4",
    "vue-router": "^2.7.0",
    "weex-vue-render": "^0.12.14"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^3.1.0",
    "postcss-cssnext": "^3.0.2",
    "serve": "^6.0.2",
    "vue-loader": "^10.3.0",
    "vue-progressbar": "^0.7.1",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^1.15.0",
    "weex-devtool": "^0.2.80",
    "weex-loader": "^0.5.3",
    "weex-vue-loader": "^0.4.2"
  },
  "optionalDependencies": {
    "ios-deploy": "^1.9.0"
  }
}
